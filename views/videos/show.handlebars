<div class="contents-container">
  <div class="video-card">
      <a href="/videos/{{video._id}}/edit" id="edit" class="input-button">Edit</a>
    <div>
      <a href="/videos/{{video._id}}/delete" id="edit" class="input-button">Delete</a>
    </div>
    <div class="video-title"><h1>{{ video.title }}</h1></div>
    <iframe class="video-player" src="{{ video.url }}" frameborder="0" allowfullscreen></iframe>
    <div class="video-description">
      <p>{{ video.description }}</p>
    </div>
  </div>
</div>
